{
  "name": "react-statement",
  "version": "0.0.2",
  "description": "Declarative statement (if-else / for / try-catch) components for React",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist lib es coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tslint -p .",
    "test": "jest --colors",
    "test:coverage": "npm test -- --coverage",
    "docs:start": "cd docs && npm start",
    "docs:build": "cd docs && npm run build",
    "build:es": "tsc --outDir es -d",
    "build:commonjs": "tsc -m commonjs --outDir lib",
    "build": "npm-run-all -p build:*",
    "prepare": "npm run clean && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yenshih/react-statement.git"
  },
  "keywords": [],
  "authors": [
    "Shi Yan <yenshih95@gmail.com> (https://github.com/yenshih)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yenshih/react-statement/issues"
  },
  "npmName": "react-statement",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "eslint": "^5.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "stylelint": "^9.3.0",
    "stylelint-order": "^0.8.1",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^16.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
