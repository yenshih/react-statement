      "node_modules",
      "test"
    ],
    "testURL": "https://localhost",
    "silent": true
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "eslint": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^16.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
